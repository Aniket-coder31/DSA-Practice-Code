class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
        int start=0, end=nums.length-1;
        int count=0, max=Integer.MIN_VALUE;
        for(int i=start;i<=end;i++){
            if(nums[i]==1){
                count++;
            } else{
                count=0;
            }
            max=Math.max(count,max);
        }
        return max;
    }
}
