public class a{
    public static void printSubArray(int arr[]){
        int start=0;int end=arr.length;
        for(int i=start;i<end;i++){
            for(int j=i+1;j<end;j++){
                System.out.print("("+arr[i]+","+arr[j]+")");
                System.out.print(" ");
            }
            start++;
            System.out.println();
        }
    }

    public static void main(String args[]){
        int arr[]={8,4,6,9,1,3,5};
        printSubArray(arr);
    }
}
